        linear 0.1 xoffset -2 yoffset 2
        linear 0.1 xoffset 3 yoffset -3
        linear 0.1 xoffset 2 yoffset -2
        linear 0.1 xoffset -3 yoffset 3
        linear 0.1 xoffset 0 yoffset 0
        repeat

    transform my_shake2:
        linear 0.01 xoffset -2 yoffset 2
        linear 0.01 xoffset 3 yoffset -3
        linear 0.01 xoffset 2 yoffset -2
        linear 0.01 xoffset -3 yoffset 3
        linear 0.01 xoffset 0 yoffset 0
        repeat

transform chase:
    linear 0.0 xoffset 0
    block:
        linear 0.45 xoffset -70
        linear 0.45 xoffset 140
        linear 0.45 xoffset -70
        linear 0.45 xoffset 140
        linear 0.45 xoffset -70
        linear 0.45 xoffset 140
        repeat

transform jumping:
    linear 0.06 yoffset -20
    linear 0.06 yoffset 0

transform fall:
    linear 0.5 yoffset 730

